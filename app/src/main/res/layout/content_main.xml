<?xml version="1.0" encoding="utf-8"?>
　<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.hjgolf.shinichiro.hajimetegolf.MainActivity"
    tools:showIn="@layout/activity_main"
    android:background="@color/abc_primary_text_disable_only_material_dark"
    android:clickable="false"><![CDATA[





    ImageButton


        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:maxWidth="60dp"
        android:maxHeight="150dp"
        android:id="@+id/imageButton"
        android:src="@drawable/wani"
        android:layout_below="@+id/imageView"
        android:layout_alignParentStart="true" />
        android:scaletype="center"




    ]]>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="　基本メニュー　"
        android:id="@+id/button2"
        android:textSize="@dimen/abc_dialog_padding_top_material"
        android:background="@android:color/holo_green_light"
        android:onClick="activity_basic"
        android:layout_centerVertical="true"
        android:layout_alignStart="@+id/button4" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="　７日間練習メニュー　"
        android:id="@+id/button3"
        android:textSize="@dimen/abc_dialog_padding_top_material"
        android:background="@android:color/holo_green_light"
        android:textColor="@color/abc_input_method_navigation_guard"
        android:onClick="daysmen"
        android:layout_below="@+id/textView2"
        android:layout_centerHorizontal="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="　はじめに　"
        android:id="@+id/button"
        android:textSize="@dimen/abc_dialog_padding_top_material"
        android:background="@android:color/holo_green_light"
        android:onClick="activity_prolog"
        android:layout_below="@+id/imageView2"
        android:layout_centerHorizontal="true" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView2"
        android:src="@drawable/wani"
        android:maxWidth="40dp"
        android:maxHeight="120dp"
        android:layout_below="@+id/progressBar"
        android:layout_centerHorizontal="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="　パッティング　"
        android:id="@+id/button4"
        android:textSize="@dimen/abc_dialog_padding_top_material"
        android:background="@android:color/holo_green_light"
        android:layout_below="@+id/textView3"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="練習前に必ずチェックして下さい"
        android:id="@+id/textView2"
        android:textColor="@color/abc_primary_text_disable_only_material_light"
        android:layout_below="@+id/button2"
        android:layout_alignStart="@+id/textView10" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="１週間で基本的なスウィングを作ります"
        android:id="@+id/textView3"
        android:textColor="@color/abc_primary_text_disable_only_material_light"
        android:layout_below="@+id/button3"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="自宅で毎日練習して下さい"
        android:id="@+id/textView4"
        android:textColor="@color/abc_primary_text_disable_only_material_light"
        android:layout_below="@+id/button4"
        android:layout_alignEnd="@+id/button3" />

    <ProgressBar
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/progressBar"
        android:background="@color/colorPrimaryDark"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="HajimeteGolf"
        android:id="@+id/textView"
        android:textColor="@color/abc_primary_text_disable_only_material_dark"
        android:textSize="@dimen/abc_text_size_display_1_material"
        android:layout_above="@+id/imageView2"
        android:layout_alignEnd="@+id/button3" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="練習のポイントを説明します"
        android:id="@+id/textView10"
        android:textColor="@color/abc_primary_text_disable_only_material_light"
        android:layout_below="@+id/button"
        android:layout_centerHorizontal="true" />


</RelativeLayout>
